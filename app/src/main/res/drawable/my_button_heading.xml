<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#adafd8" />
            <gradient android:angle="-90" android:startColor="#666982" android:endColor="#6c527a"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#6c527a" />
            <solid android:color="#666982"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#8587aa" />
            <gradient android:angle="-90" android:startColor="#666982" android:endColor="#6c527a" />
        </shape>
    </item>
</selector>
